{
    "cli": {
        "version": ">= 7.0.0"
    },
    "build": {
        "development": {
            "developmentClient": true,
            "distribution": "internal"
        },
        "preview": {
            "distribution": "internal"
        },
        "production": {
            "autoIncrement": true
        }
    },
    "submit": {
        "production": {
            "ios": {
                "appleId": "${EXPO_APPLE_ID}",
                "ascAppId": "YOUR_APP_STORE_CONNECT_APP_ID",
                "appleTeamId": "YOUR_APPLE_TEAM_ID"
            },
            "android": {
                "serviceAccountKeyPath": "./google-play-key.json",
                "track": "internal"
            }
        }
    }
}